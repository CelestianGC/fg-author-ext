<root>
  <!-- use my export.lua file -->
  <windowclass name="export" merge="join">
    <script file="utility/scripts/export_author.lua" merge="replace"/>
    <sheetdata>
			<stringcontrol name="label_excludecheck">
				<anchored to="list" position="aboveright" offset="15,3" />
				<font>sheetlabel</font>
				<lineoffset default="on">-1</lineoffset>
				<static textres="AUTHOR_export_label_exclude_ro" />
        <tooltip textres="AUTHOR_export_exclude_ro_tooltip" />
			</stringcontrol>
      
    </sheetdata>
  </windowclass>
  
  <windowclass name="export_categoryentry" merge="join">
    <sheetdata>
			<button_checkbox name="neverreadonly">
        <anchored to="label" position="righthigh" offset="-20" width="20" height="20" />
			</button_checkbox>      
      
    </sheetdata>
  </windowclass>
 </root>
